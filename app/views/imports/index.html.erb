<% if session[:clear_import_decisions] %>
  <script>
    const importData = <%= session.delete(:clear_import_decisions).to_json.html_safe %>;
    const storageKey = `${importData.type}_${importData.id}_decisions`;
    localStorage.removeItem(storageKey);
  </script>
<% end %>

<%= render "imports/header" %>

<%= render AppImportsNavigationComponent.new(active: @active, team: current_team) %>

<%= render AppImportsTableComponent.new(team: current_team, uploaded_files: uploaded_files_view?) %>
