<% content_for :before_main do %>
  <%= govuk_back_link(href: schools_team_path) %>
<% end %>

<%= h1 "Find a school to add to your team" %>

<%= form_with(model: @draft_school, url: wizard_path, method: :put) do |f| %>
  <%= f.govuk_text_field :urn,
                         label: {
                           text: "School URN",
                         },
                         width: 10 %>
  <%= f.govuk_submit "Continue" %>
<% end %>
