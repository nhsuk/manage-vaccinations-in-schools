<%= render AppCardComponent.new do |c| %>
  <% c.with_heading { "Session details" } %>
  <%= govuk_summary_list classes: "app-summary-list--no-bottom-border" do |summary_list|
        summary_list.with_row do |row|
          row.with_key { "Programmes" }
          row.with_value { programmes }
        end
      
        summary_list.with_row do |row|
          row.with_key { "Session dates" }
          row.with_value { session_dates }
        end
      
        if consent_requests.present?
          summary_list.with_row do |row|
            row.with_key { "Consent requests" }
            row.with_value { consent_requests }
          end
        end
      
        if consent_reminders.present?
          summary_list.with_row do |row|
            row.with_key { "Reminders" }
            row.with_value { consent_reminders }
          end
        end
      end %>
<% end %>
